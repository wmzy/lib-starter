{
  "name": "lib-starter",
  "version": "0.0.0",
  "private": true,
  "description": "lib-starter",
  "main": "dist/lib-starter.cjs.js",
  "module": "dist/lib-starter.esm.js",
  "browser": "dist/lib-starter.umd.js",
  "unpkg": "dist/lib-starter.umd.min.js",
  "jsdelivr": "dist/lib-starter.umd.min.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "commit": "lint-staged && git-cz -n",
    "build": "rollup -c",
    "prepublish": "rimraf dist && npm run build",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint --fix src test *.js",
    "test": "cross-env NODE_ENV=test nyc mocha -r @babel/register --recursive --exclude mock --exclude fixtures -r should -r should-sinon",
    "start": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wmzy/lib-starter.git"
  },
  "keywords": [
    "lib",
    "starter"
  ],
  "author": "wmzy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wmzy/lib-starter/issues"
  },
  "homepage": "https://github.com/wmzy/lib-starter",
  "engines": {
    "node": ">=8.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/register": "^7.6.0",
    "babel-eslint": "^10.0.3",
    "commitizen": "^4.0.3",
    "coveralls": "^3.0.6",
    "cross-env": "^5.2.1",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-builtin-compat": "0.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-mocha": "^6.1.1",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "rollup": "^1.21.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-by-output": "^1.0.0",
    "rollup-plugin-terser": "^5.1.2",
    "should": "^13.2.3",
    "should-sinon": "0.0.6",
    "sinon": "^7.4.2"
  },
  "lint-staged": {
    "*.js": [
      "cross-env NODE_ENV=production eslint --fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "lodash": "^4.17.15"
  }
}
